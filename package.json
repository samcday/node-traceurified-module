  "repository": {
    "type": "git",
    "url": "https://github.com/samcday/node-traceurified-module.git"
  },
  "keywords": [
    "traceur",
    "es6",
    "harmony",
    "ecmascript6",
    "esnext",
    "jsnext"
  ],
  "author": "Sam Day <me@samcday.com.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samcday/node-traceurified-module/issues"
  },
  "homepage": "https://github.com/samcday/node-traceurified-module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "mkdirp": "^0.5.0",
    "traceur": "0.0.72"
  },
  "jshintConfig": {
    "node": true,
    "unused": true,
