{
  "name": "traceurified-module",
  "version": "0.0.1",
  "description": "Write Node.js packages in ES6.",
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "bin": {
    "traceurified-module-compile": "bin/traceurified-module-compile.js"
